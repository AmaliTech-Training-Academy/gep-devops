{
  "serviceName": "notification-service",
  "cluster": "event-planner-prod-cluster",
  "taskDefinition": "event-planner-prod-notification-service",
  "loadBalancers": [
    {
      "targetGroupArn": "arn:aws:elasticloadbalancing:REGION:ACCOUNT:targetgroup/NAME",
      "containerName": "notification-service",
      "containerPort": 8085
    }
  ],
  "desiredCount": 2,
  "launchType": "FARGATE",
  "platformVersion": "LATEST",
  "networkConfiguration": {
    "awsvpcConfiguration": {
      "subnets": ["subnet-xxxxx", "subnet-yyyyy"],
      "securityGroups": ["sg-xxxxx"],
      "assignPublicIp": "DISABLED"
    }
  },
  "healthCheckGracePeriodSeconds": 60,
  "schedulingStrategy": "REPLICA",
  "deploymentConfiguration": {
    "maximumPercent": 200,
    "minimumHealthyPercent": 100,
    "deploymentCircuitBreaker": {
      "enable": true,
      "rollback": true
    }
  },
  "serviceRegistries": [
    {
      "registryArn": "arn:aws:servicediscovery:REGION:ACCOUNT:service/srv-xxxxx"
    }
  ],
  "placementStrategies": [
    {
      "type": "spread",
      "field": "attribute:ecs.availability-zone"
    }
  ],
  "enableECSManagedTags": true,
  "propagateTags": "SERVICE"
}
